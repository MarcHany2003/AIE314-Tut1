    {
        "type": "NarrativeText",
        "element_id": "dc1c936e19fdb4458ad430f1709734e9",
        "text": "core gan concept it's basically two neural networks fighting . the generator acts like a counterfeiter trying to create fake data, and the discriminator acts like a cop trying to tell the difference between the fake data and the real data . they train against each other until the fakes look real.",
        "metadata": {
            "coordinates": {
                "points": [
                    [
                        72.024,
                        149.92399999999998
                    ],
                    [
                        72.024,
                        212.80399999999997
                    ],
                    [
                        524.5079999999999,
                        212.80399999999997
                    ],
                    [
                        524.5079999999999,
                        149.92399999999998
                    ]
                ],
                "system": "PixelSpace",
                "layout_width": 595.32,
                "layout_height": 841.92
            },
            "file_directory": "data",
